	<https://data.geoscience.earth/id/dataset/borehole> a dcat:Dataset;
		dct:title "Borehole data index";
		dct:identifier "https://data.geoscience.earth/id/dataset/borehole";
		adms:identifier [ a adms:Identifier;
			adms:schemeAgency "DDSS-ID";
			skos:notation "WP15-DDSS-001a";
		]; 
		dct:description "This dataset is the harvesting of Borehole Index content provided by the current data providers. It provides metadata information for each borehole (ID, purpose, location, type of observations / measurements).  It aims at supporting search and download of borehole datasets.  The detailed information along the boreholes are not provided and are not planned to be consolidated in a central database, but rather be accessed through another set of services, to be delivered by the data providers (ie national geological surveys or research organisation).";
		dct:accrualPeriodicity "http://purl.org/cld/freq/irregular"^^xsd:anyURI;
		dct:created "2016-10-27"^^xsd:date;
		dct:temporal 
			[ a dct:PeriodOfTime;
				schema:startDate "1886-05-16T00:00:00Z"^^xsd:dateTime; 
				#schema:endDate "YYYY-MM-DDThh:mm:ssZ"^^xsd:dateTime; 
			]; 
	dct:spatial [ a dct:Location; 
		locn:geometry "POLYGON((-180 -90,-180 90,180 90,180 -90,-180 -90))"^^gsp:wktLiteral;
	]; 
		dcat:theme <category:beholes>;
		dcat:keyword "borehole","geology","INSPIRE:GE","GeoSciML";
		dcat:contactPoint <contactBoreholeIndex/manager>;
		dcat:distribution <https://data.geoscience.earth/id/dataset/borehole/distributionWfs>; 
		dcat:distribution <https://data.geoscience.earth/id/dataset/borehole/distributionWms>; 
		dct:publisher <https://data.geoscience.earth/ncl/organization/1>;
		dqv:hasQualityAnnotation [ a oa:Annotation ;
			oa:hasBody "https://data.geoscience.earth/doc/Quality_assurance/EPOS_TCS_GIM_quality_assurance.html"^^xsd:anyURI ;
		];
. 
