			rdfs:range "xsd:float";
      			rdfs:label "minLat";
			schema:minValue "-90" ;
      			schema:maxValue "90" ;
			schema:defaultValue "35.60371874069731";
			hydra:required "true"^^xsd:boolean;
		];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxLon"^^xsd:string;
      			hydra:property "epos:easternmostLongitude" ;
			rdfs:range "xsd:float";
      			rdfs:label "maxLon";
			schema:minValue "-180" ;
      			schema:maxValue "180" ;
			schema:defaultValue "27.070";
			hydra:required "true"^^xsd:boolean;
	 	];
		hydra:mapping[ a hydra:IriTemplateMapping;
			hydra:variable "maxLat"^^xsd:string;
      			hydra:property "epos:northernmostLatitude" ;
			rdfs:range "xsd:float";
      			rdfs:label "maxLat";
			schema:minValue "-9" ;
      			schema:maxValue "90" ;
			schema:defaultValue "68.0075";
			hydra:required "true"^^xsd:boolean;
		];
	];
